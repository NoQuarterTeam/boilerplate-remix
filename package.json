{
  "private": true,
  "name": "boilerplate-remix",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config ./build/server.js",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./app/lib/server.ts --outdir=build",
    "build": "run-s build:*",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "generate": "prisma generate",
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx app",
    "lint": "eslint --config .eslintrc.commit.js --ext .ts,.tsx app",
    "pre-commit": "lint-staged",
    "prettify": "prettier --write app",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "eslint --fix --config .eslintrc.commit.js"
    ],
    "*.(js|jsx|json|ts|tsx|html)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.17",
    "@chakra-ui/react": "2.5.2",
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "@prisma/client": "4.11.0",
    "@remix-run/express": "1.14.3",
    "@remix-run/node": "1.14.3",
    "@remix-run/react": "1.14.3",
    "@remix-run/server-runtime": "1.14.3",
    "@sendgrid/client": "7.7.0",
    "@sendgrid/mail": "7.7.0",
    "aws-sdk": "2.1338.0",
    "bcrypt": "5.1.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "framer-motion": "7.10.3",
    "handlebars": "4.7.7",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "nodemailer": "6.9.1",
    "prisma": "4.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-icons": "4.8.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@remix-run/dev": "1.14.3",
    "@remix-run/eslint-config": "1.14.3",
    "@types/bcrypt": "5.0.0",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "9.0.1",
    "@types/morgan": "1.9.4",
    "@types/nodemailer": "6.4.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "esbuild": "0.17.12",
    "esbuild-register": "3.4.2",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-simple-import-sort": "9.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.6",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "prettier": "2.8.6"
  },
  "sideEffects": false
