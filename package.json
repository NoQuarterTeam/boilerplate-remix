{
  "private": true,
  "name": "boilerplate-remix",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config ./build/server.js",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./app/lib/server.ts --outdir=build",
    "build": "run-s build:*",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "generate": "prisma generate",
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx app",
    "lint": "eslint --config .eslintrc.commit.js --ext .ts,.tsx app",
    "pre-commit": "lint-staged",
    "prettify": "prettier --write app",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "eslint --fix --config .eslintrc.commit.js"
    ],
    "*.(js|jsx|json|ts|tsx|html)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.4",
    "@chakra-ui/react": "^2.2.3",
    "@emotion/cache": "11.10.0",
    "@emotion/react": "11.10.0",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@prisma/client": "4.1.1",
    "@remix-run/express": "1.6.7",
    "@remix-run/node": "1.6.7",
    "@remix-run/react": "1.6.7",
    "@remix-run/server-runtime": "1.6.7",
    "@sendgrid/client": "7.7.0",
    "@sendgrid/mail": "7.7.0",
    "aws-sdk": "2.1185.0",
    "bcrypt": "5.0.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dayjs": "1.11.4",
    "dotenv": "16.0.1",
    "framer-motion": "6.5.1",
    "handlebars": "4.7.7",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "nodemailer": "6.7.7",
    "prisma": "4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "14.2.2",
    "react-icons": "4.4.0",
    "zod": "3.17.10"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@remix-run/dev": "1.6.7",
    "@remix-run/eslint-config": "1.6.7",
    "@types/bcrypt": "5.0.0",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.8",
    "@types/morgan": "1.9.3",
    "@types/nodemailer": "6.4.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "esbuild": "0.14.51",
    "esbuild-register": "3.3.3",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "prettier": "2.7.1"
  },
  "sideEffects": false
}
