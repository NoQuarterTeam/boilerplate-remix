{
  "private": true,
  "name": "boilerplate-remix",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "remix dev",
    "build": "remix build",
    "generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db push --force-reset --preview-feature && node --require esbuild-register app/prisma/seed.ts",
    "postinstall": "prisma generate && remix setup node",
    "start": "remix-serve ./api/_build",
    "db:push": "prisma db push",
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx app",
    "prettify": "prettier --write app",
    "typecheck": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "vercel-build": "yarn prisma:generate && remix build",
    "prisma:generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate"
  },
  "prisma": {
    "schema": "app/prisma/schema.prisma"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "eslint --fix --config .eslintrc.commit.js"
    ],
    "*.(js|jsx|json|ts|tsx|html)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "1.8.7",
    "@emotion/cache": "11.7.1",
    "@emotion/react": "11.8.2",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.8.1",
    "@prisma/client": "3.11.1",
    "@remix-run/react": "1.3.4",
    "@remix-run/serve": "1.3.4",
    "@remix-run/vercel": "1.3.3",
    "@sendgrid/client": "7.6.2",
    "@sendgrid/mail": "7.6.2",
    "aws-sdk": "2.1101.0",
    "bcrypt": "5.0.1",
    "dayjs": "1.11.0",
    "dotenv": "16.0.0",
    "framer-motion": "6.2.8",
    "handlebars": "4.7.7",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "6.7.3",
    "prisma": "3.11.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "12.0.4",
    "react-icons": "4.3.1",
    "remix": "1.3.3",
    "zod": "3.14.3"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@remix-run/dev": "1.3.4",
    "@types/bcrypt": "5.0.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/nodemailer": "6.4.4",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.1",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "resolutions": {
    "prettier": "2.6.1"
  },
  "sideEffects": false
}
